SELECT group_concat(TABLE_NAME,";",COLUMN_NAME,"\r\n") FROM Information_Schema.COLUMNS where TABLE_SCHEMA = 'hotel'

#query to get username and password
SELECT group_concat(host,";",user,":",password,"\r\n") FROM mysql.user
